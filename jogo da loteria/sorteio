$sorteado = rand  (00,$max);
$nun_sorteado = [];
 $numero_dezenas = [];
 

for ($i=1; $i <= 60; $i++) { 
    
    
for ($j=0; $j <6 ; $j++) { 
    
    if ($nun_sorteado [j] == $sorteado ) {
        stop;
    }else{
        $nun_sorteado[] = $sorteado;
    }
    
    
    
}
   
print ("$sorteado \n");
}

